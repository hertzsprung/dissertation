FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X36c/9c";
projection2D XZ;

FieldsToPlot (
    [ExnerDiff rawValues -0.00008 0.00008 0.00001 "$GMTU/colours/red_white_blue.cpt"]
    [Uf vectors 50 1 "blue"]
    [nonlinearMomentum vectors 8000 1 "red"]
    [none mesh "gray"]
);
annotate "minMax 0.01 -0.005";

region "-30e3/30e3/0/1e3";
boundaryMarks "30000/100";
