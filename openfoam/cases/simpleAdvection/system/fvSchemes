FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

divSchemes {
    default         none;
//    div(phi,T)      Gauss vanLeer01; // Matt Jones' scheme
    div(phi,T)      Gauss TVDlimited cubicUpwindCPCFit 3;
}

gradSchemes {
    default         Gauss linear;
}

laplacianSchemes {
    default         Gauss linear corrected;
}

ddtSchemes {
    default         backward;
}

fluxRequired {
    default         no;
    T;
}
