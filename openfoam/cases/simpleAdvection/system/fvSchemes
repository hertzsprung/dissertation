FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

divSchemes {
    default         none;
    div(phi,T)      Gauss linearUpwind grad(T);
}

gradSchemes {
    default         Gauss linear;
}

laplacianSchemes {
    default         Gauss linear corrected;
}

ddtSchemes {
    default         Euler;
}

