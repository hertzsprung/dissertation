FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

application     simpleAdvection;
startFrom       startTime;
startTime       0;
stopAt          endTime;
endTime         21600; // 6 hours
deltaT          100;
writeControl    runTime;
writeInterval   300; // every 5 minutes
