FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}

dimensions      [1 0 0 0 0 0 0]; // tracer in kg

internalField	uniform 0;

boundaryField {
	inlet {
		type    fixedValue;
		value   uniform 1;
	}

	outlet {
		type zeroGradient;
	}

	ground {
		type	fixedValue;
		value	uniform 0;
	}

	upperLid {
		type    zeroGradient;
	}

	frontAndBack {
		type empty;
	}
}
