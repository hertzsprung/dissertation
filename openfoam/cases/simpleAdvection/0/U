FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

// u = 1 ms^-1
internalField   uniform (1 0 0);

boundaryField {
	inlet {
		type zeroGradient;
	}

	outlet {
		type zeroGradient;
	}

	ground {
		type zeroGradient;
	}

	upperLid {
		type zeroGradient;
	}

	frontAndBack {
		type empty;
	}
}
