mesh_noOrography_CASE := openfoam/cases/mesh/noOrography

mesh_noOrography_BLOCK_MESH_DICT := $(mesh_noOrography_CASE)/$(BLOCK_MESH_DICT)

mesh_noOrography_MESH_PATHS := $(addprefix $(mesh_noOrography_CASE)/,$(MESH_FILES))

$(eval $(call MESH_TEMPLATE,$(mesh_noOrography_CASE),mesh-noOrography))

.INTERMEDIATE: mesh-noOrography.intermediate

$(mesh_noOrography_MESH_PATHS): mesh-noOrography.intermediate

mesh-noOrography.intermediate: $(mesh_noOrography_BLOCK_MESH_DICT)
	$(BLOCK_MESH) -dict $(BLOCK_MESH_DICT) -case $(mesh_noOrography_CASE)

